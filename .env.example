# ICDA Prototype - Environment Configuration
# Copy this to .env and fill in your values

# AWS Configuration (uses default profile if not set)
AWS_PROFILE=default
AWS_REGION=us-east-1

# Bedrock Model IDs
NOVA_MICRO_MODEL=us.amazon.nova-micro-v1:0
NOVA_PRO_MODEL=us.amazon.nova-pro-v1:0

# Optional: Redis (leave empty for in-memory cache)
REDIS_URL=

# Demo Mode (set to false to use real Bedrock)
DEMO_MODE=false
